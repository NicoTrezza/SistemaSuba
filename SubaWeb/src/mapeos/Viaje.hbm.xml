<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.Viaje" table="Viaje">
		<id column="idViaje" name="idViaje">
			<generator class="identity"/>
		</id>
		<property name="fechaHora" column="fechaHora" type="calendar"/>
		<property name="valor" column="valor" type="float"/>
		<many-to-one name = "tarjeta" class = "datos.Tarjeta"
		column = "idTarjeta" not-null = "true"/>
		<many-to-one name = "maquina" class = "datos.Maquina"
		column = "idMaquina" not-null = "true"/>
	</class>
</hibernate-mapping>