<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.Tarjeta" table="Tarjeta">
		<id column="idTarjeta" name="idTarjeta">
			<generator class="identity"/>
		</id>
		<property name="nroTarjeta" column="nroTarjeta" type="int"/>
		<property name="saldo" column="saldo" type="float"/>
		<property name="activa" column="activa" type="boolean"/>
				
	</class>
</hibernate-mapping>