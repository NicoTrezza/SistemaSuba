<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.Carga" table="Carga">
		<id column="idCarga" name="idCarga">
			<generator class="identity"/>
		</id>
		<property name="nroTarjeta" column="nroTarjeta" type="int"/>
		<property name="fechaHora" column="fechaHora" type="calendar"/>
				
	</class>
</hibernate-mapping>