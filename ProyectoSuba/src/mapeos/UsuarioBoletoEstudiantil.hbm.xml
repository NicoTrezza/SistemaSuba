<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.UsuarioBoletoEstudiantil" table="UsuarioBoletoEstudiantil">
		<id column="idUsuarioBoletoEstudiantil" name="idUsuarioBoletoEstudiantil">
			<generator class="identity"/>
		</id>
		<property name="fechaVencimiento" column="fechaVencimiento" type="calendar"/>
		<many-to-one name = "usuario" class = "datos.Usuario"
		column = "idUsuario" not-null = "true"/>
		<many-to-one name = "boletoEstudiantil" class = "datos.BoletoEstudiantil"
		column = "idBoletoEstudiantil" not-null = "true"/>
	</class>
</hibernate-mapping>