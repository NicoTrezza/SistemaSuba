<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.BoletoColectivo" table="BoletoColectivo">
		<id column="idBoletoColectivo" name="idBoletoColectivo">
			<generator class="identity"/>
		</id>
		<property name="seccion" column="seccion" type="int"/>
		<property name="valor" column="valor" type="float"/>		
	</class>
</hibernate-mapping>